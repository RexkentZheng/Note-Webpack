{
	"presets": ["react", "env", "stage-0"],
	"env": {
		"development": {
			"plugins": [
				"transform-decorators-legacy",
				"transform-class-properties",
				["react-transform", {
					"transforms": [{
						"transform": "react-transform-hmr",
						"imports": ["react"],
						"locals": ["module"]
					}]
				}],
				["import", [{"libraryName": "antd", "style": "css"}]]
			]
		}
	}
}